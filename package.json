{
  "name": "test-runtime",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run rollup",
    "clean": "rimraf build",
    "clean-install": "rimraf node_modules package-lock.json && npm install",
    "docker-build": "docker build -t mariuslundgard/test-runtime .",
    "docker-run": "docker run --rm --name test-runtime --publish 3000:3000 mariuslundgard/test-runtime",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "precommit": "npm run lint && npm run typecheck && npm test",
    "rollup": "npm-run-all rollup-*",
    "rollup-client": "cross-env NODE_PATH=src/lib rollup --config .rollup/client.config.js",
    "rollup-worker": "cross-env NODE_PATH=src/lib rollup --config .rollup/worker.config.js",
    "rollup-server": "cross-env NODE_PATH=src/lib rollup --config .rollup/server.config.js",
    "start": "node scripts/start",
    "test": "cross-env NODE_PATH=src/lib jest",
    "typecheck": "flow check",
    "watch": "cross-env NODE_PATH=src/lib node scripts/dev"
  },
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariuslundgard/test-runtime.git"
  },
  "bugs": {
    "url": "https://github.com/mariuslundgard/test-runtime/issues"
  },
  "homepage": "https://github.com/mariuslundgard/test-runtime#readme",
  "engineStrict": true,
  "engines": {
    "node": "8.9.1"
  },
  "dependencies": {
    "@transclusion/request": "^1.0.0-alpha.1",
    "@transclusion/runtime-browser": "^1.0.0-alpha.2",
    "@transclusion/runtime-core": "^1.0.0-alpha.2",
    "@transclusion/runtime-server": "^1.0.0-alpha.2",
    "@transclusion/runtime-worker": "^1.0.0-alpha.2",
    "@transclusion/vdom": "^1.0.0-beta.3",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "path-to-regexp": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chokidar": "^2.0.0",
    "cross-env": "^5.1.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "npm-run-all": "^4.1.2",
    "postcss-color-function": "^4.0.1",
    "postcss-css-variables": "^0.8.0",
    "postcss-import": "^11.0.0",
    "postcss-modules": "^1.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^1.2.8",
    "rollup-plugin-uglify": "^3.0.0",
    "supertest": "^3.0.0"
  }
}
